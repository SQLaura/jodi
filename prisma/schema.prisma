generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = "file:database.sqlite"
}

model User {
  id              String  @id
  drop_enabled    Boolean @default(false)
  grab_enabled    Boolean @default(false)
  series_enabled  Boolean @default(false)
  quest_enabled   Boolean @default(false)
  drop            String  @default("0")
  grab            String  @default("0")
  series          String  @default("0")
  quest           String  @default("0")
}
